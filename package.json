{
  "name": "form-control",
  "version": "0.0.1",
  "scripts": {
    "build-pages": "ng build --project=playground --prod --base-href=https://joster422.github.io/form-control/",
    "build-lib": "ng build form-control && npm run copy-license",
    "push-pages": "git push origin --delete gh-pages && git subtree push --prefix dist/playground origin gh-pages",
    "copy-license": "powershell Copy-Item \".\\LICENSE\" -Destination \".\\dist\\form-control\"",
    "npm-pack": "cd dist/form-control && npm pack",
    "package": "npm run build-lib && npm run npm-pack",
    "pages": "npm run build-pages && npm run push-pages"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "~9.1.7",
    "@angular/common": "~9.1.7",
    "@angular/compiler": "~9.1.7",
    "@angular/core": "~9.1.7",
    "@angular/forms": "~9.1.7",
    "@angular/platform-browser": "~9.1.7",
    "@angular/platform-browser-dynamic": "~9.1.7",
    "@angular/router": "~9.1.7",
    "rxjs": "~6.5.5",
    "tslib": "^2.0.0",
    "zone.js": "~0.10.3"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.901.6",
    "@angular-devkit/build-ng-packagr": "~0.901.6",
    "@angular/cli": "~9.1.6",
    "@angular/compiler-cli": "~9.1.7",
    "@angular/language-service": "~9.1.7",
    "@types/jasmine": "~3.5.10",
    "@types/jasminewd2": "~2.0.3",
    "@types/node": "^14.0.1",
    "codelyzer": "^5.2.2",
    "jasmine-core": "~3.5.0",
    "jasmine-spec-reporter": "~5.0.2",
    "karma": "~5.0.7",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage-istanbul-reporter": "~3.0.2",
    "karma-jasmine": "~3.1.1",
    "karma-jasmine-html-reporter": "^1.5.4",
    "ng-packagr": "^9.1.3",
    "protractor": "~7.0.0",
    "ts-node": "~8.10.1",
    "tslint": "~6.1.2",
    "typescript": "^3.8.3"
  }
}
