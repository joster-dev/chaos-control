{
  "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
  "version": 1,
  "newProjectRoot": "projects",
  "projects": {
    "form-control": {
      "root": "projects/form-control",
      "sourceRoot": "projects/form-control/src",
      "projectType": "library",
      "prefix": "fc",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-ng-packagr:build",
          "options": {
            "tsConfig": "projects/form-control/tsconfig.lib.json",
            "project": "projects/form-control/ng-package.json"
          }
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "main": "projects/form-control/src/test.ts",
            "tsConfig": "projects/form-control/tsconfig.spec.json",
            "karmaConfig": "projects/form-control/karma.conf.js"
          }
        },
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": [
              "projects/form-control/tsconfig.lib.json",
              "projects/form-control/tsconfig.spec.json"
            ],
            "exclude": ["**/node_modules/**"]
          }
        }
      }
    },
    "fc-test": {
      "root": "projects/fc-test/",
      "sourceRoot": "projects/fc-test/src",
      "projectType": "application",
      "prefix": "app",
      "schematics": {},
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:browser",
          "options": {
            "outputPath": "dist/fc-test",
            "index": "projects/fc-test/src/index.html",
            "main": "projects/fc-test/src/main.ts",
            "polyfills": "projects/fc-test/src/polyfills.ts",
            "tsConfig": "projects/fc-test/tsconfig.app.json",
            "assets": [
              "projects/fc-test/src/favicon.ico",
              "projects/fc-test/src/assets"
            ],
            "styles": ["projects/fc-test/src/styles.css"],
            "scripts": [],
            "es5BrowserSupport": true
          },
          "configurations": {
            "production": {
              "fileReplacements": [
                {
                  "replace": "projects/fc-test/src/environments/environment.ts",
                  "with": "projects/fc-test/src/environments/environment.prod.ts"
                }
              ],
              "optimization": true,
              "outputHashing": "all",
              "sourceMap": false,
              "extractCss": true,
              "namedChunks": false,
              "aot": true,
              "extractLicenses": true,
              "vendorChunk": false,
              "buildOptimizer": true,
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "2mb",
                  "maximumError": "5mb"
                }
              ]
            }
          }
        },
        "serve": {
          "builder": "@angular-devkit/build-angular:dev-server",
          "options": {
            "browserTarget": "fc-test:build"
          },
          "configurations": {
            "production": {
              "browserTarget": "fc-test:build:production"
            }
          }
        },
        "extract-i18n": {
          "builder": "@angular-devkit/build-angular:extract-i18n",
          "options": {
            "browserTarget": "fc-test:build"
          }
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "main": "projects/fc-test/src/test.ts",
            "polyfills": "projects/fc-test/src/polyfills.ts",
            "tsConfig": "projects/fc-test/tsconfig.spec.json",
            "karmaConfig": "projects/fc-test/karma.conf.js",
            "styles": ["projects/fc-test/src/styles.css"],
            "scripts": [],
            "assets": [
              "projects/fc-test/src/favicon.ico",
              "projects/fc-test/src/assets"
            ]
          }
        },
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": [
              "projects/fc-test/tsconfig.app.json",
              "projects/fc-test/tsconfig.spec.json"
            ],
            "exclude": ["**/node_modules/**"]
          }
        }
      }
    },
    "fc-test-e2e": {
      "root": "projects/fc-test-e2e/",
      "projectType": "application",
      "prefix": "",
      "architect": {
        "e2e": {
          "builder": "@angular-devkit/build-angular:protractor",
          "options": {
            "protractorConfig": "projects/fc-test-e2e/protractor.conf.js",
            "devServerTarget": "fc-test:serve"
          },
          "configurations": {
            "production": {
              "devServerTarget": "fc-test:serve:production"
            }
          }
        },
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": "projects/fc-test-e2e/tsconfig.e2e.json",
            "exclude": ["**/node_modules/**"]
          }
        }
      }
    }
  },
  "defaultProject": "form-control"
}
